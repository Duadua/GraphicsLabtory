materialparameter castshadow 1
beginpass shadow
shader shadow_instance.vs shadow.fs
endpass
beginpass lightloop
shader normal_instance.vs glb_normal_emission.fs
passparameter glb_unif_AlbedoTex streakedmarble_basecolor.dds
passparameter glb_unif_RoughnessTex streakedmarble_roughness.dds
passparameter glb_unif_MetallicTex streakedmarble_metallic.dds
passparameter glb_unif_NormalTex streakedmarble_normal.dds
passparameter glb_unif_EmissionTex streakedmarble_emission.bmp
passparameter glb_unif_SpecularPFCLOD 7.0
passparameter glb_unif_DiffusePFCTex diffuse.pfc
passparameter glb_unif_SpecularPFCTex specular.pfc
passparameter glb_unif_EmissionIntensity 10.0 10.0 1.0
endpass